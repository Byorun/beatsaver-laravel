{
  "name": "beatsaver-mp3-convert",
  "version": "1.0.0",
  "description": "Convert audio files to MP3 for previewing.",
  "main": "build/index.js",
  "scripts": {
    "test": "npm run check && npm run lint",
    "check": "npm run build -- --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "rimraf build",
    "build": "tsc",
    "build-dev": "npm run build -- --sourceMap",
    "watch": "tsc --watch",
    "dev": "npm run watch -- --sourceMap"
  },
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.2",
    "extract-zip": "^1.6.7",
    "glob": "^7.1.3",
    "p-queue": "^3.0.0",
    "rimraf": "^2.6.3",
    "yargs": "^13.2.0"
  },
  "devDependencies": {
    "@types/extract-zip": "^1.6.2",
    "@types/glob": "^7.1.1",
    "@types/node": "^11.9.4",
    "@types/p-queue": "^3.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/yargs": "^12.0.9",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3"
  }
}
